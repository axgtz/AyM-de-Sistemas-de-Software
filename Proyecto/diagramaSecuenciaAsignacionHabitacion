@startuml
actor Usuario


Usuario -> Interface: buscarHabitacion()
activate Interface
database Servidor
Interface -> Servidor: buscarHabitacion()
activate Servidor

Servidor --> Interface: String - Habitacion
deactivate Servidor
Interface --> Usuario: String - Habitacion
deactivate Interface


Usuario -> Interface: buscarAlumno()
activate Interface

Interface -> Servidor: buscarAlumno()
activate Servidor

Servidor --> Interface: String - Nombre Alumno
deactivate Servidor
Interface --> Usuario: String - Nombre Alumno
deactivate Interface


Usuario -> Interface: asignarHabitacion()
activate Interface

Interface -> Servidor: asignarHabitacion()
activate Servidor

Servidor --> Interface: Verdadero

Interface --> Usuario: Verdadero
@enduml
