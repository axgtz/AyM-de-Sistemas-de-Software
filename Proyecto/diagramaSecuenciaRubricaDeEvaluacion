@startuml
actor Usuario

Usuario -> Interface: buscarPrefecto()
activate Interface
database Servidor
activate Servidor
Interface -> Servidor: buscarPrefecto()

Servidor --> Interface: String - Nombre Prefecto
deactivate Servidor
Interface --> Usuario: String - Nombre Prefecto
deactivate Interface


Usuario -> Interface: buscarAlumno()
activate Interface
activate Servidor

Interface -> Servidor: buscarAlumno()

Servidor --> Interface: String - Nombre Alumno
deactivate Servidor
Interface --> Usuario: String - Nombre Alumno
deactivate Interface

Usuario -> Interface: alta()
activate Interface
Interface -> Servidor: alta()
activate Servidor
Servidor --> Interface: Verdadero

Interface --> Usuario: Verdadero
@enduml
