@startuml
actor Usuario

Usuario -> Interface: buscarAlumno()
activate Interface
database Servidor
Interface -> Servidor: buscarAlumno()

Servidor --> Interface: String - Nombre Alumno
deactivate Servidor
Interface --> Usuario: String - Nombre Alumno
deactivate Interface

Usuario -> Interface: contactarAlumno()
activate Interface

Interface -> Servidor: contactarAlumno()

Servidor --> Interface: Verdadero

Interface --> Usuario: Verdadero
@enduml
